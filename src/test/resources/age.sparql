PREFIX : <https://fakeschema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?name ?age WHERE {
  [] :hasPet [] ;
     :name ?name ;
     :birthDate ?birthDate .

  BIND(NOW() AS ?now)
  BIND(STRDT(STR(?birthDate), xsd:date) AS ?birth)
  BIND(YEAR(?now) - YEAR(?birth) - IF( MONTH(?now)<MONTH(?birth) || (MONTH(?now)=MONTH(?birth) && DAY(?now)<DAY(?birth)),1,0 ) as ?age)
} ORDER BY ASC(?name)